{
  "extends": [
    "eslint:recommended",
    "plugin:es5/no-es2016"
  ],
  "plugins": [
    "es5"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "no-cond-assign": 0,
    "no-constant-condition": 0,
    "no-prototype-builtins": 0
  }
}
